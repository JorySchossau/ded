
P = $linear {
		vary entity = $markov_brain {
			vary inputs = 5
			vary outputs = 2
			vary hiddens = 2
			}
		vary size = 50
		vary track-lineage = true
}

combine = $collate {
		vary x1 = $forager {
			vary  sensor-range = 5
			vary replace = true
 		}
		vary x2 = $max_one 
}

E = $two_cycle {
		vary fitness =   !combine 
    	vary count = 301 
		vary cout-frequency = 10
		vary snapshot-frequency = 50
		vary pop-avmax-frequency = 1
    	vary selector = $elite_replacement {
			vary strength = 0.4
			pre value = score
		} 
} 

